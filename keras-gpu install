sudo dpkg -i cuda-repo-ubuntu1604-8-0-local_8.0.44-1_amd64.deb 

sudo pip install tensorflow-gpu==1.3.0

sudo apt install git

sudo pip install opencv-python numpy scipy scikit-learn pillow h5py keras imgaug sklearn Pillow



git clone https://github.com/bstriner/keras_adversarial.git
cd keras_adversarial
sudo python setup.py install

sudo apt-get install python-imaging

sudo pip install jupyter

sudo add-apt-repository ppa:webupd8team/sublime-text-3
sudo apt-get update
sudo apt-get install sublime-text-installer
